{
  "extends": "@beesley/tsconfig/lambda.json",
  "compilerOptions": {
    "types": ["node"],
    "outDir": "dist",
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "noUnusedParameters": false
  },
  "include": ["src/**/*.ts", "src/*.ts"],
  "exclude": ["src/*.test.*", "src/**/*.test.*", "src/test/*.ts"]
}
